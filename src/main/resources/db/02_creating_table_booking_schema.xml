<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="2" author="mike">
        <createTable tableName="bookings">

            <column name="id" type="VARCHAR(50)">
                <constraints nullable="false" primaryKey="true" unique="true"/>
            </column>
            <column name="employeeId" type="VARCHAR(50)">
                <constraints nullable="true"/>
            </column>
            <column name="clientId" type="VARCHAR(50)">
                <constraints nullable="true"/>
            </column>
            <column name="beginningTime" type="TIMESTAMP">
                <constraints nullable="true"/>
            </column>
            <column name="finishingTime" type="TIMESTAMP">
                <constraints nullable="true"/>
            </column>
            <column name="service" type="VARCHAR(50)">
                <constraints nullable="true"/>
            </column>
            <column name="bookingStatus" type="VARCHAR(50)">
                <constraints nullable="true"/>
            </column>

        </createTable>
    </changeSet>
</databaseChangeLog>